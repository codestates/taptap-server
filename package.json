{
  "name": "taptap-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node ./app.js",
    "test": "NODE_ENV=test node ./app.js",
    "precommit": "NODE_ENV=production lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "ignore":  [
    "node_modules/",
    "**/spec/*.js"
  ],
  "dependencies": {
    "body-parser": "latest",
    "cookie-parser": "latest",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.5",
    "sequelize": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  }
}
